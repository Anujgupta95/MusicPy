{
  "version":2,
  "framework": "python-flask",
  "build": "pip install -r requirements.txt && python app.py",
  "builds":[
    {
      "src":"./app.py"
      "use": "@vercel/python",
      "config":{"runtime":"python3.6"}
    }
  ],
  "routes":[
    {
      "src":"/(.*)",
      "dest":"/"
    }
  ]
}
